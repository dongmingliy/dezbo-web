.col-md-3.col-sm-4
  .thumbnail.centered
    figure
      img.img-responsive(src='{{i.imageURL}}', width='263', height='500', alt='')
    .item-box
      .text-center
        h5 {{i.celebName}}
        h3.item-name {{i.itemName}}
        h5 SUPPORTING
        p.text-small.charity-name {{i.charityName}}
        a(href='{{i.link}}')
          button.btn.btn-primary.item-box-button.centered ENTER TO WIN
        h6.countdown Ends {{i.ending}}

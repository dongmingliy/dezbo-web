block content
  .homeItems(ng-controller="homeCtrl")
    .container
      .divider
        i.fa.fa-star.blue-raffle
      .row
        h3.text-center.text-uppercase.htitle ENTER TO WIN
        rafflebox(ng-repeat='item in raffleitems', i='item')
    .container
      .divider
        i.fa.fa-star.yellow-bidding
      .row
        h3.text-center.text-uppercase.htitle START BIDDING
        biddingbox(ng-repeat='item in biddingitems', i='item')
